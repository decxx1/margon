---
import Feature from '@/components/Feature.astro';
---
<section class="bg-white dark:bg-gray-800">
    <div class="py-8 px-4 mx-auto max-w-screen-xl sm:py-16 lg:px-6">
        <div class="max-w-screen-md mb-8 lg:mb-16 text-center mx-auto">
            <h2 class="mb-4 text-4xl tracking-tight font-extrabold text-primary-950 dark:text-white">Productos y servicios alimenticios de calidad</h2>
            <p class="font-light text-primary-900 dark:text-gray-400 sm:text-xl">Contamos con una ámplia variedad de productos alimenticios, aditivos y bebidas</p>
        </div>
        <div class="mx-auto justify-items-center grid grid-cols-1 sm:grid-cols-2 md:grid-cols-4 md:gap-8 xl:gap-12">
            <Feature 
                title='Salados'
                items={[
                    'Sopas y caldos deshidratados',
                    'Salsas y purés instantaneos'
                ]}
                icon="soup"
            />
            <Feature 
                title='Dulces'
                items={[
                    'Polvos para preparar postres y bebidas',
                    'Repostería varios',
                    'Golosinas',
                ]}
                icon="dessert"
            /> 
            <Feature 
                title='Bebidas con alcohol'
                items={[
                    'Cócteles',
                    'Amargos y sangrías',
                ]}
                icon="drink"
            /> 
            <Feature 
                title='Importados'
                items={[
                    'Esencias',
                    'Condimentos y aditivos',
                ]}
                icon="import"
            />     
        </div>
    </div>
</section>

